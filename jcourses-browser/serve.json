		{ "source": "/api/:a/:b/:c", "destination": "http://localhost\\:3002/api/:a/:b/:c" },
		{ "source": "/api/:a/:b/:c/:d", "destination": "http://localhost\\:3002/api/:a/:b/:c/:d" },
		{ "source": "/api/:a/:b/:c/:d/:e", "destination": "http://localhost\\:3002/api/:a/:b/:c/:d/:e" }
